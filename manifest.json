{
  "manifest_version": 3,
  "name": "Netflix Shakti API Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["https://www.netflix.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "js": ["https://code.jquery.com/jquery-3.6.0.min.js", "content_script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
